                <% if (!login_id) { %>
                    <li><a href="/login">Login</a></li>
                <% } else { %>
                    <li><a href="/logout">Logout</a></li>
                <% } %>
            </ul>
        </nav>
    </header>

    <h1>Welcome to Our Website</h1>

    <p>This is the home page content.</p>

</body>
</html>
